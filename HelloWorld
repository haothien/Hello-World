# Hello-World I am XXX
import java.util.Scanner; 
public class myName 
{ 
    public static void main(String[] args) 
    { 
         
        Scanner sc = new Scanner(System.in); 
  
         
        String name = sc.nextLine(); 
  
         
  
         
        System.out.println("Hello World my name is "+name); 
         
    } 
} 
